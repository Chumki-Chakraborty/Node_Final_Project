          <div class="container">
            <h1 style="text-align: center; background-color: aqua;">User Course Application Details..</h1>
            <table class="table" border="1px" style="text-align: center; ">
              <thead>
                <tr>
                  <th scope="col">Id</th>
                  <th scope="col">UserName</th>
                  <th scope="col">Email</th>
                  <th scope="col">Date_of_Birth</th>
                  <th scope="col">Course_Name</th>
                  <th scope="col">Qualification</th>
                  <th scope="col">Education_Mode</th>
                  <th scope="col">Date</th>
                </tr>
              </thead>
              <tbody>
                <% let id=0 %>
                <% for(User of UserCourseApply){%>
                  <tr>
                    <th scope="row"><%=++id%></th>
                    <td><%=User.User_id.name%></td>
                    <td><%=User.User_id.email%></td>
                    <td><%=User.Date_of_Birth%></td>
                    <td><%=User.Course_Name.CourseName%></td>
                    <td><%=User.Qualification%></td>
                    <td><%=User.Education_Mode%></td>
                    <td><%=User.date%></td>
                  </tr>
               <%}%>
                
                
                
              </tbody>
            </table>
