<body class="courses-page">

  <%-include('./Common/nav')%>
 
  <!--  -->
  <!-- Page Title -->
  <div class="page-title" data-aos="fade">
    <div class="heading">
      <div class="container">
        <div class="row d-flex justify-content-center text-center">
          <div class="col-lg-8">
            <h1>User Dashboard</h1>
           
          </div>
        </div>
      </div>
    </div>
    <nav class="breadcrumbs">
      <div class="container">
        <ol>
          <li><a href="/user/home">Home</a></li>
          <li class="current">Dashboard</li>
        </ol>
      </div>
    </nav>
  </div>
  <!-- End Page Title -->
  <!--  -->
  <br>
  <main class="main">
        <div class="container-fluid">
        <h1 style="text-align: center; background-color: darkgray; font-weight: bolder;">User Application submit Details</h1>
    <table class="table" border="1px" style="text-align: center;">
      <thead>
        <tr>
          <th scope="col">ID</th>
          <th scope="col">User Name</th>
          <th scope="col">User Email</th>
          <th scope="col">Date_of_Birth</th>
          
          <th scope="col">Course_Name</th>
          <th scope="col">Qualification</th>
          <th scope="col">Education_Mode</th>
          <th scope="col">Date</th>
        </tr>
      </thead>
      <tbody>
        <% let id=0 %>
        <% for(alldata of ApplyCourse){%>
          <tr>
            <td><%=++id%></td>
           <td><%=alldata.User_id.name%></td>
           <td><%=alldata.User_id.email%></td>
           <td><%=alldata.Date_of_Birth%></td>
           <td><%=alldata.Course_Name.CourseName%></td>
           <td><%=alldata.Qualification%></td>
           <td><%=alldata.Education_Mode%></td>
           <td><%=alldata.date%></td>
           </tr>
        <%}%>
       


        </tbody>
    </table>
  </div>
<!-- **************** -->
      
